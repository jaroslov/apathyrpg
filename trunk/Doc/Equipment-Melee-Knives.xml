        </field>
        <field name="Cost" table="yes">
          <text>0</text>
        </field>
        <field name="Damage" table="yes">
          <text>
           <roll>
            <num>1</num>
            <face>6</face>
            <bOff>+</bOff>
            <bns>2</bns>
          </roll> 
        </text>
        </field>
        <field name="Hands" table="yes">
          <text>1</text>
        </field>
        <field name="Minimum Strength" table="yes">
          <text>8</text>
